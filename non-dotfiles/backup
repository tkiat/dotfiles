alias 2tmux-theme1="\
	tmux split-window -v -p 30 && \
	tmux split-window -h -p 35"

alias 2open-tmux-backup="\
tmux new-session \; \
	send-keys 'mc' C-m \; \
	split-window -v -p 30 \; \
	send-keys '2gt_music' C-m \; \
	send-keys 'cmus' C-m \; \
	send-keys '5' \; \
	split-window -h -p 35 \; \
	send-keys '2start-pomodoro.py' C-m \;"
