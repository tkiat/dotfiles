#!/usr/bin/env dash
config_dir=~/Git/dotfiles-and-configs
# alias
[ ! -f ~/.alias.specific ] && ln -sf $config_dir/alias/.alias.guix ~/.alias.specific
# bash
[ ! -f ~/.bash_profile ] && ln -sf $config_dir/bash/.bash_profile ~
# channels
[ ! -f ~/.config/guix/channels.scm ] && ln -sf $config_dir/guix/channels.scm ~/.config/guix
# zsh
[ ! -f ~/.zshrc ] && ln -sf $config_dir/zsh/.zshrc.guix ~/.zshrc
